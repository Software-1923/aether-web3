{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/dist/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/old-path",
      "destination": "/new-path",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, must-revalidate, private"
        }
      ]
    }
  ],
  "map": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
