{
  "version": 2,
  "routes": [
    "/": {
      "file": "src/index.tsx",
      "dataRoute": "data"
    },
    "/about": {
      "file": "pages/about.js",
      "dataRoute": "data"
    },
    "/contact": {
      "file": "pages/contact.js",
      "dataRoute": "data"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
